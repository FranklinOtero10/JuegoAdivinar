        android:supportsRtl="true"
        android:theme="@style/Theme.EO17I04001"
        tools:targetApi="31">
        <activity
            android:name=".InfoJuegoActivity"
            android:exported="false" />
        <activity
            android:name=".PuntajesAltosActivity"
            android:exported="false" />
